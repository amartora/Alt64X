void wifiScanOne (String bullet[50], int g){
  
  ttgo->tft->fillScreen(TFT_BLACK); //clear screen
 
  drawHome(65520, &FreeMono9pt7b, ML_DATUM);

  ttgo->tft->setCursor(0, 50);//reset cursor
  
  for (int i = 0; i < g; ++i) {
        ttgo->tft->println(bullet [i]);//print first 5 results
  }
  x=-1;
  y=-1;
  
}
  
